{
    "name": "felipe-lara-reforged",
    "version": "1.0.1",
    "description": "Personal & Portfolio Website",
    "repository": {
        "url": "https://github.com/Aosiee/aosiee.github.io.git",
        "type": "git"
    },
    "author": "Felipe Lara",
    "license": "MIT",
    "dependencies": {
        "@dogstudio/highway": "^2.2.1",
        "gsap": "^3.10.4",
        "imagesloaded": "^5.0.0",
        "parcel": "^2.5.0",
        "yarn": "^1.22.18"
    },
    "gh-pages-deploy": {
        "staticpath": "dist",
        "cname": "felipe-lara.com",
        "commit": "Website Auto Deploy",
        "noprompt": false
    },
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "preinstall": "npx npm-force-resolutions",
        "dev": "parcel ./src/index.html",
        "prebuild": "npx rimraf build",
        "build": "parcel build --public-url ./ ./src/index.html",
        "watch": "parcel watch ./src/index.html",
        "deploy": "git subtree push --prefix dist origin gh-pages-live-2",
        "deploy2": "gh-pages -b master -d build",
        "clean-source": "rimraf README.md src webroot package.json"
    },
    "devDependencies": {
        "@parcel/transformer-sass": "^2.5.0",
        "gh-pages": "^4.0.0",
        "gh-pages-deploy": "^0.1.1",
        "posthtml-doctype": "^1.1.1",
        "posthtml-include": "^1.7.3"
    }
}
