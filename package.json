{
    "name": "felipe-lara-reforged",
    "version": "1.0.1",
    "description": "Personal & Portfolio Website",
    "repository": "https://github.com/Aosiee/aosiee.github.io.git",
    "author": "Felipe Lara",
    "license": "MIT",
    "dependencies": {
        "@dogstudio/highway": "^2.2.1",
        "gsap": "^3.10.4",
        "imagesloaded": "^5.0.0",
        "parcel": "^2.5.0",
        "yarn": "^1.22.18"
    },
    "gh-pages-deploy": {
        "staticpath": "dist",
        "cname": "felipe-lara.com",
        "commit": "Website Auto Deploy",
        "noprompt": false
    },
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "preinstall": "npx npm-force-resolutions",
        "dev": "parcel ./src/index.html",
        "prebuild-dev": "shx rm -rf dist/*",
        "build-dev": "parcel build ./src/index.html --no-optimize --public-url ./",
        "prebuild": "shx rm -rf dist/*",
        "build": "parcel build ./src/index.html --public-url ./",
        "push-gh-pages": "push-dir --dir=dist --branch=gh-pages-live-5 --cleanup --verbose",
        "clean-source": "rimraf README.md src webroot package.json",
        "predeploy": "parcel build ./src/index.html --public-url ./",
        "deploy": "gh-pages -d dist"
    },
    "devDependencies": {
        "@parcel/transformer-sass": "^2.5.0",
        "gh-pages": "^4.0.0",
        "gh-pages-deploy": "^0.1.1",
        "parcel-bundler": "^1.12.5",
        "posthtml-doctype": "^1.1.1",
        "posthtml-include": "^1.7.3",
        "shx": "^0.3.2"
    }
}